{
  "name": "@ekoppen/cloudbox-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for CloudBox BaaS platform - Complete type-safe client for collections, storage, users, and functions",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run lint && npm run type-check && npm run build"
  },
  "keywords": [
    "cloudbox",
    "baas",
    "backend-as-a-service",
    "api",
    "typescript",
    "sdk",
    "client",
    "database",
    "collections",
    "storage",
    "file-upload",
    "authentication",
    "users",
    "serverless",
    "functions",
    "rest-api",
    "javascript",
    "node",
    "browser",
    "vibcode"
  ],
  "author": {
    "name": "VibCode",
    "email": "info@vibcode.com",
    "url": "https://vibcode.com"
  },
  "maintainers": [
    {
      "name": "ekoppen",
      "email": "eelko@vibcode.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekoppen/cloudbox.git",
    "directory": "sdk/typescript"
  },
  "bugs": {
    "url": "https://github.com/ekoppen/cloudbox/issues"
  },
  "homepage": "https://github.com/ekoppen/cloudbox/tree/main/sdk/typescript#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "rollup": "^3.29.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}